configuration {
	modi:               "window,run,drun";
	font:               "FiraCode Nerd Font Mono 12";
	show-icons:         false;
	terminal:           "kitty";
	matching:           "fuzzy";
	sort:               true;
	case-sensitive:     false;
	sorting-method:     "fzf";
	kb-primary-paste:   "Alt+v";
	kb-clear-line:      "Alt+l";
	kb-mode-next:       "Alt+i";
	kb-mode-previous:   "Alt+h";
	kb-row-up:          "Alt+e";
	kb-row-down:        "Alt+n";
}
@theme "/usr/share/rofi/themes/sidebar.rasi"
